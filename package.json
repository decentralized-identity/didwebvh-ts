{
  "name": "didwebvh-ts",
  "module": "dist/esm/index.js",
  "type": "module",
  "version": "1.0.0",
  "main": "dist/cjs/index.cjs",
  "browser": "dist/browser/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "dev": "bun --watch --inspect-wait ./src/resolver.ts",
    "server": "bun --watch ./src/resolver.ts",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:bail": "bun test --watch --bail --verbose",
    "test:log": "mkdir -p ./test/logs && LOG_RESOLVES=true bun test &> ./test/logs/test-run.txt",
    "cli": "bun src/cli.ts",
    "build": "bun run scripts/build.ts",
    "build:clean": "rm -rf dist",
    "prepublishOnly": "bun run build"
  },
  "devDependencies": {
    "@types/bun": "^1.1.14",
    "bun-bagel": "^1.1.0",
    "bun-types": "latest",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "semantic-release": "^22.0.0"
  },
  "peerDependencies": {
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@noble/curves": "^1.6.0",
    "@noble/ed25519": "^2.1.0",
    "bs58": "^6.0.0",
    "elysia": "^0.8.17",
    "json-canonicalize": "^1.0.6",
    "multiformats": "^13.3.1",
    "nanoid": "^5.0.8"
  },
  "bin": {
    "didwebvh": "./dist/cli/didwebvh.js"
  },
  "exports": {
    ".": {
      "browser": "./dist/browser/index.js",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs",
      "types": "./dist/types/index.d.ts"
    }
  }
}
